@model ChatApp.Controllers.PerformanceController.Status

<div>
    <form asp-controller="Performance" asp-action="GenerateUsers">
        <label for="userCount">Count of users to generate</label>
        <input type="number" id="userCount" name="count" placeholder="Count of users to generate"/>
        <button type="submit">Generate</button>
    </form>
</div>

<div>
    <p>Postgres processing time: @Model.PostgresTime</p>
    <p>Redis processing time: @Model.RedisTime</p>
    
    <form asp-controller="Performance" asp-action="CalculatePostgres">
        <button type="submit">Calculate performance</button>
    </form>
</div>

